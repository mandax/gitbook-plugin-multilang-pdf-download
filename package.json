{
  "name": "gitbook-plugin-multilang-pdf-download",
  "version": "1.0.0",
  "description": "GitBook plugin to specify download pdf links",
  "main": "index.js",
  "engines": {
    "gitbook": ">2.5.0"
  },
  "gitbook": {
    "properties": {
      "base": {
        "required": true,
        "title": "URL base to PDF",
        "type": "string"
      },
      "fileName": {
        "default": "file_${lang}.pdf",
        "required": true,
        "title": "PDF file name base",
        "type": "string"
      },
      "label": {
        "default": "Download PDF",
        "title": "PDF button label",
        "type": [
          "string",
          "object"
        ]
      }
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mandax/gitbook-plugin-multilang-pdf-download.git"
  },
  "keywords": [
    "gitbook",
    "plugin",
    "download",
    "pdf"
  ],
  "author": "Anderson Ferreira Pinto (mandax)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mandax/gitbook-plugin-multilang-pdf-download/issues"
  },
  "homepage": "https://github.com/mandax/gitbook-plugin-multilang-pdf-download#readme",
  "dependencies": {
    "lodash": "^4.17.4"
  }
}
